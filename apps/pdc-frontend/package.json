    "@frameless/utils": "0.1.0",
    "@nl-design-system-unstable/documentation": "1.8.0",
    "@open-formulieren/sdk": "3.3.0",
    "@playwright/test": "1.45.3",
    "@tanstack/react-query": "4.29.25",
    "@utrecht/component-library-css": "8.1.4",
    "@utrecht/component-library-react": "10.3.8",
    "@utrecht/design-tokens": "3.4.1",
    "@utrecht/web-component-library-react": "3.0.10",
    "@utrecht/open-forms-container-react": "1.0.4",
    "@utrecht/open-forms-container-css": "2.0.0",
    "@utrecht/flo-legal-decision-tree-client": "1.0.0",
    "@utrecht/flolegal-decision-tree-css": "2.0.0",
    "accept-language": "3.0.18",
    "classnames": "2.3.3",
    "csp-header": "5.2.1",
    "downshift": "7.6.2",
    "i18next": "22.5.1",
    "i18next-browser-languagedetector": "7.0.2",
    "i18next-resources-to-backend": "1.1.4",
    "is-absolute-url": "4.0.1",
    "lodash.mergewith": "4.6.2",
    "next": "13.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "12.3.1",
    "react-loading-skeleton": "3.3.1",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.1.0",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@types/jest": "29.5.12",
    "@types/lodash.mergewith": "4.6.9",
    "@utrecht/focus-ring-css": "3.0.0",
