    "@utrecht/spotlight-section-css": "2.0.0",
    "@utrecht/multiline-data-css": "2.0.0",
    "@utrecht/textbox-css": "3.0.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "sass": "1.63.6",
    "ts-node": "10.9.2",
    "wait-on": "7.2.0"
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi.git",
    "directory": "apps/pdc-frontend"
