{
  "name": "@frameless/cms",
  "version": "0.0.0-semantically-released",
  "description": "",
  "private": true,
  "workspaces": [
    "packages/upl",
    "packages/strapi-plugin-uniform-product-name",
    "packages/strapi-tiptap-editor",
    "packages/catalogi-data",
    "packages/samenwerkende-catalogi",
    "apps/pdc-dashboard",
    "apps/pdc-frontend",
    "packages/*"
  ],
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [],
  "scripts": {
    "start": "npm run --workspaces start --if-present",
    "build": "npm run --workspaces build --if-present",
    "build:frontend": "npm run build --workspace @frameless/${APP}",
    "build:strapi": "npm run build --workspace @frameless/${APP}",
    "build:both": "npm run build --workspace @frameless/${APP1} && npm run build --workspace @frameless/${APP2}",
    "format": "prettier --write .",
    "dev:frontend": "npm run dev -w @frameless/${APP}",
    "dev:strapi": "npm run dev -w @frameless/${APP}",
    "dev:both": "concurrently \"npm run dev -w @frameless/${APP1}\" \"npm run dev -w @frameless/${APP2}\"",
    "development": "node scripts/start.js",
    "start:frontend": "npm run start -w @frameless/${APP}",
    "start:strapi": "npm run start -w @frameless/${APP}",
    "start:both": "concurrently \"npm run start -w @frameless/${APP1}\" \"npm run start -w @frameless/${APP2}\"",
    "test": "npm run test --workspaces --if-present",
    "clean": "npm run --workspaces clean --if-present",
    "clean-node_modules": "npm exec --workspaces --include-workspace-root -- rimraf node_modules",
    "lint": "npm-run-all lint:*",
    "lint-fix": "npm-run-all lint-fix:*",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:md": "markdownlint '**/*.md'",
    "lint:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx' --report-unused-disable-directives .",
    "lint:package-json": "npmPkgJsonLint '**/package.json'",
    "lint:prettier": "prettier --check .",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "lint-fix:css": "stylelint --fix '**/**/*.{css, scss}'",
    "lint-fix:js": "eslint --ext '.js,.json,.jsx,.ts,.tsx' --fix --report-unused-disable-directives .",
    "release": "multi-semantic-release --ignore-packages=apps/pdc-dashboard/**,apps/pdc-frontend/**",
    "lint-fix:md": "markdownlint --fix '**/*.md'",
    "prettier": "prettier --list-different --write .",
    "postinstall": "patch-package"
  },
  "author": "",
  "license": "",
  "engines": {
    "node": "18.x.x"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.21.8",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@strapi-community/eslint-config": "0.4.3",
    "@types/node": "20.1.2",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "concurrently": "8.0.1",
    "enquirer": "2.4.1",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "lodash": "4.17.21",
    "markdownlint-cli": "0.34.0",
    "multi-semantic-release": "3.0.2",
    "npm-package-json-lint": "6.4.0",
    "npm-run-all": "4.1.5",
    "patch-package": "7.0.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "stylelint": "15.6.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard-scss": "9.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.0.4"
  }
}
