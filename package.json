{
  "name": "@frameless/cms",
  "version": "0.0.0-semantically-released",
  "description": "",
  "private": true,
  "workspaces": [
    "packages/strapi-tiptap-editor",
    "packages/catalogi-data",
    "packages/samenwerkende-catalogi",
    "apps/strapi-dashboard",
    "apps/frontend",
    "packages/**"
  ],
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [],
  "scripts": {
    "start": "npm run --workspaces start --if-present",
    "build": "npm run --workspaces build --if-present",
    "format": "prettier --write .",
    "dev": "concurrently \"npm run dev -w @frameless/strapi-dashboard\" \"npm run dev -w @frameless/frontend\"",
    "start:dev": "concurrently \"npm run start -w @frameless/strapi-dashboard\" \"npm run start -w @frameless/frontend\"",
    "test": "npm run test --workspaces --if-present",
    "clean": "npm run --workspaces clean --if-present",
    "clean-node_modules": "npm exec --workspaces --include-workspace-root -- rimraf node_modules",
    "lint": "npm-run-all lint:*",
    "lint-fix": "npm-run-all lint-fix:*",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:md": "markdownlint '**/*.md'",
    "lint:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx' --report-unused-disable-directives .",
    "lint:package-json": "npmPkgJsonLint '**/package.json'",
    "lint:prettier": "prettier --check .",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "lint-fix:css": "stylelint --fix '**/**/*.{css, scss}'",
    "lint-fix:js": "eslint --ext '.js,.json,.jsx,.ts,.tsx' --fix --report-unused-disable-directives .",
    "release": "multi-semantic-release --ignore-packages=apps/strapi-dashboard/**,apps/frontend/**",
    "lint-fix:md": "markdownlint --fix '**/*.md'",
    "prettier": "prettier --list-different --write ."
  },
  "author": "",
  "license": "",
  "engines": {
    "node": "18.x.x"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.3",
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@strapi-community/eslint-config": "0.4.3",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "babel-eslint": "10.1.0",
    "concurrently": "8.0.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "lodash": "4.17.21",
    "markdownlint-cli": "0.33.0",
    "multi-semantic-release": "3.0.2",
    "npm-package-json-lint": "6.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.7",
    "rimraf": "5.0.0",
    "stylelint": "15.4.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard-scss": "7.0.1",
    "stylelint-order": "6.0.3",
    "typescript": "5.0.4"
  }
}
