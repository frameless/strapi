{
  "name": "@frameless/cms",
  "version": "0.0.0-semantically-released",
  "description": "",
  "private": true,
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [],
  "scripts": {
    "start": "turbo run start",
    "start:pdc-frontend": "pnpm --filter @frameless/pdc-frontend start",
    "start:pdc-dashboard": "pnpm --filter @frameless/pdc-dashboard start",
    "start:kennisbank-frontend": "pnpm --filter @frameless/kennisbank-frontend start",
    "start:kennisbank-dashboard": "pnpm --filter @frameless/kennisbank-dashboard start",
    "start:pdc-sc": "pnpm --filter @frameless/pdc-sc start",
    "build": "turbo run build",
    "build:frontend": "pnpm --filter @frameless/${APP}",
    "build:strapi": "pnpm --filter @frameless/${APP}",
    "build:both": "pnpm --filter @frameless/${APP1} && pnpm --filter @frameless/${APP2}",
    "dev:frontend": "pnpm --filter @frameless/${APP} dev",
    "dev:strapi": "pnpm --filter @frameless/${APP} dev",
    "dev:both": "concurrently \"pnpm --filter @frameless/${APP1} dev\" \"pnpm --filter @frameless/${APP2} dev\"",
    "development": "node scripts/start.js",
    "start:frontend": "pnpm --filter @frameless/${APP}",
    "start:strapi": "pnpm --filter @frameless/${APP}",
    "start:both": "concurrently \"pnpm --filter @frameless/${APP1}\" \"pnpm --filter @frameless/${APP2}\"",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "lint": "npm-run-all lint:*",
    "lint-fix": "npm-run-all lint-fix:*",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:md": "markdownlint '**/*.md'",
    "lint:js": "NODE_OPTIONS=--max-old-space-size=8048 eslint --ext '.{js, json, jsx, ts, tsx}' --report-unused-disable-directives .",
    "lint:package-json": "npmPkgJsonLint '**/package.json'",
    "lint:prettier": "prettier --check .",
    "lint-fix:prettier": "prettier --write .",
    "lint-fix:css": "stylelint --fix '**/*.{css,scss}'",
    "lint-fix:js": "NODE_OPTIONS=--max-old-space-size=8048 eslint --ext '.{js, json, jsx, ts, tsx}' --fix --report-unused-disable-directives .",
    "release": "multi-semantic-release --ignore-packages='apps/*/**'",
    "lint-fix:md": "markdownlint --fix '**/*.md'",
    "prettier": "prettier --list-different --write ."

  },
  "author": "",
  "license": "",
  "engines": {
    "node": "20.x.x",
    "pnpm": "8.x.x"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.21.8",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/node": "20.1.2",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "concurrently": "8.0.1",
    "enquirer": "2.4.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "lodash": "4.17.21",
    "markdownlint-cli": "0.34.0",
    "multi-semantic-release": "3.0.2",
    "npm-package-json-lint": "7.0.0",
    "npm-run-all": "4.1.5",
    "patch-package": "8.0.0",
    "prettier": "3.0.2",
    "rimraf": "5.0.5",
    "stylelint": "15.10.3",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard-scss": "10.0.0",
    "stylelint-order": "6.0.3",
    "turbo": "1.11.3",
    "typescript": "5.0.4"
  }
}
