{
  "name": "@frameless/provider-upload-vercel",
  "version": "0.0.0-semantically-released",
  "description": "Vercel provider for strapi upload",
  "private": true,
  "keywords": [
    "vercel",
    "blob",
    "upload",
    "strapi",
    "provider"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi.git"
  },
  "author": "Frameless",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "watch": "rollup --config rollup.config.ts --configPlugin typescript -w",
    "clean": "rimraf dist .rollup.cache"
  },
  "dependencies": {
    "@vercel/blob": "0.12.5",
    "into-stream": "8.0.1",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/fs-extra": "11.0.4",
    "rollup": "3.29.4",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.36.0"
  },
  "engines": {
    "node": "20.x.x"
  }
}
