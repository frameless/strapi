{
  "name": "@frameless/samenwerkende-catalogi",
  "version": "1.0.0",
  "private": false,
  "sideEffects": true,
  "author": "@frameless",
  "description": "A Nodejs application that generate the samenwerkende-catalogi XML",
  "license": "",
  "keywords": [],
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "./dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@frameless/catalogi-data": "file:../catalogi-data",
    "xmlbuilder2": "3.0.2",
    "@frameless/upl": "file:../upl"
  },
  "scripts": {
    "prebuild": "pnpm clean",
    "builds": "rollup -c ",
    "build": "pnpm build:browser && pnpm build:node",
    "build:browser": "tsc -p ./tsconfig.browser.json",
    "build:node": "tsc -p ./tsconfig.node.json",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": "20.x.x",
    "pnpm": "8.x.x"
  }
}
